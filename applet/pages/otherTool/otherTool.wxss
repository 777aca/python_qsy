.codeWrap {
    background: #fff;
    border-radius: 16rpx;
    display: flex;
    justify-content: space-between;
    margin-top: 8rpx;
    width: 100%;
}

.codeImg {
    height: 100rpx;
    width: 100rpx;
}

.codeWord {
    align-items: center;
    box-sizing: border-box;
    padding: 0 40rpx;
    width: 100%;
}

.keyWord {
    color: #00f;
    font-size: 16px;
}