<view>
    <cu-custom bgColor="{{globalColor}}" bind:__l="__l" isBack="{{true}}" vueId="5e4520a8-1" vueSlots="{{['content']}}">
        <view slot="content">视频解析</view>
    </cu-custom>
    <ad adTheme="white" adType="video" unitId="{{ad}}" wx:if="{{showAd}}"></ad>
    <view>
        <video class="video-play" poster="{{cover}}" src="{{videoUrl}}"></video>
    </view>
    <cmd-progress bind:__l="__l" custom="{{true}}" percent="{{percent}}" strokeColor="white" successPercent="{{percent}}" vueId="5e4520a8-2" vueSlots="{{['default']}}">
        <text style="color:#009688;">{{percent+'%'}}</text>
    </cmd-progress>
    <view class="box">
        <view class="cu-bar btn-group">
            <button bindtap="__e" class="cu-btn bg-orange shadow-blur" data-event-opts="{{[ [ 'tap',[ [ 'copyMvUrl',['$event'] ] ] ] ]}}">复制无水印链接</button>
            <button bindtap="__e" class="cu-btn bg-green shadow" data-event-opts="{{[ [ 'tap',[ [ 'downFile',[1,'$0'],['videoUrl'] ] ] ] ]}}">保存到手机</button>
        </view>
    </view>
    <view class="box" wx:if="{{null!=cover}}">
        <view class="cu-bar btn-group">
            <button bindtap="__e" class="cu-btn bg-blue shadow-blur" data-event-opts="{{[ [ 'tap',[ ['copyCover'] ] ] ]}}" wx:if="{{null!=cover}}">复制封面链接</button>
            <button bindtap="__e" class="cu-btn bg-grey shadow" data-event-opts="{{[ [ 'tap',[ [ 'downFile',[2,'$0'],['cover'] ] ] ] ]}}" wx:if="{{null!=cover}}">保存封面</button>
        </view>
    </view>
    <view bindtap="__e" class="padding-sm flex flex-direction" data-event-opts="{{[ [ 'tap',[ ['skipToPrize'] ] ] ]}}" style="margin-top:-7px;">
        <button class="cu-btn bg-cyan">{{prizeContent}}</button>
    </view>
    <view bindtap="__e" class="padding-sm flex flex-direction" data-event-opts="{{[ [ 'tap',[ ['back'] ] ] ]}}" style="margin-top:-7px;">
        <button class="cu-btn" style="margin-left:2px;background:#16A7F8;color:white;">返回首页继续解析</button>
    </view>
    <view class="padding-sm flex flex-direction" style="margin-top:-7px;">
        <button class="cu-btn" style="margin-left:2px;background:#16A7C4;color:white;">在线客服</button>
    </view>
    <view class="water-teach">
        <view bindtap="__e" class="tui-nickname" data-event-opts="{{[ [ 'tap',[ [ 'skipToWeb',['$event'] ] ] ] ]}}">
            <image class="tui-img-vip" src="../../static/teach.png"></image>下载失败了怎么办？ 点我查看解决方式</view>
    </view>
    <ad adTheme="white" adType="video" unitId="{{indexAd}}" wx:if="{{showAd}}"></ad>
    <privacy-popup bind:__l="__l" vueId="5e4520a8-3"></privacy-popup>
</view>
