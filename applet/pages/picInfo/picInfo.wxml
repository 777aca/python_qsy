<view>
    <cu-custom bgColor="{{globalColor}}" bind:__l="__l" isBack="{{true}}" vueId="0dd4384e-1" vueSlots="{{['content']}}">
        <view slot="content">图片去水印</view>
    </cu-custom>
    <view class="padding-sm">
        <view class="flex">
            <view class="flex-sub">
                <button bindtap="__e" class="file-btn" data-event-opts="{{[ [ 'tap',[ [ 'checkAd',[-1] ] ] ] ]}}" style="width:98%;">保存到手机相册</button>
            </view>
        </view>
        <view class="flex">
            <view class="flex-sub">
                <button bindtap="__e" class="file-btn" data-event-opts="{{[ [ 'tap',[ [ 'copyTitle',['$event'] ] ] ] ]}}" style="width:98%;">复制文案</button>
            </view>
        </view>
    </view>
    <view class="tui-product-box">
        <view class="tui-product-list">
            <view class="tui-product-container">
                <view class="tui-pro-item" hoverClass="hover" hoverStartTime="{{150}}" wx:if="{{(index+1)%2!=0}}" wx:for="{{$root.l0}}" wx:key="index">
                    <image bindtap="__e" class="tui-pro-img" data-event-opts="{{[ [ 'tap',[ [ 'previewPic',[index] ] ] ] ]}}" mode="widthFix" src="{{productList[index]}}"></image>
                    <view bindtap="__e" class="tui-pro-content" data-event-opts="{{[ [ 'tap',[ [ 'checkAd',[index] ] ] ] ]}}">
                        <view class="tui-pro-tit">下载图片</view>
                    </view>
                    <view bindtap="__e" class="tui-pro-content" data-event-opts="{{[ [ 'tap',[ [ 'copyPic',[index] ] ] ] ]}}">
                        <view class="tui-pro-tit">复制图片链接</view>
                    </view>
                    <view bindtap="__e" class="check-warp" data-event-opts="{{[ [ 'tap',[ [ 'selectImg',[index] ] ] ] ]}}">
                        <checkbox borderColor="#ffffff" checked="{{item.g0!=-1}}" value="{{index}}"></checkbox>
                    </view>
                </view>
            </view>
            <view class="tui-product-container">
                <view class="tui-pro-item" hoverClass="hover" hoverStartTime="{{150}}" wx:if="{{(index+1)%2==0}}" wx:for="{{$root.l1}}" wx:key="index">
                    <image bindtap="__e" class="tui-pro-img" data-event-opts="{{[ [ 'tap',[ [ 'previewPic',[index] ] ] ] ]}}" mode="widthFix" src="{{productList[index]}}"></image>
                    <view bindtap="__e" class="tui-pro-content" data-event-opts="{{[ [ 'tap',[ [ 'checkAd',[index] ] ] ] ]}}">
                        <view class="tui-pro-tit">下载图片</view>
                    </view>
                    <view bindtap="__e" class="tui-pro-content" data-event-opts="{{[ [ 'tap',[ [ 'copyPic',[index] ] ] ] ]}}">
                        <view class="tui-pro-tit">复制图片链接</view>
                    </view>
                    <view bindtap="__e" class="check-warp" data-event-opts="{{[ [ 'tap',[ [ 'selectImg',[index] ] ] ] ]}}">
                        <checkbox borderColor="#ffffff" checked="{{item.g1!=-1}}" value="{{index}}"></checkbox>
                    </view>
                </view>
            </view>
        </view>
    </view>
    <privacy-popup bind:__l="__l" class="vue-ref" data-ref="privacyPopup" vueId="0dd4384e-2"></privacy-popup>
</view>
