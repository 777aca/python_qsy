page {
    position: relative;
}

.small-msg {
    background: #b2b2b2;
    width: 160px;
}

.listBanner {
    align-items: center;
    background: #343547;
    border-radius: 10rpx;
    display: flex;
    justify-content: space-between;
    margin-bottom: 20rpx;
    padding: 18rpx;
}

.vip {
    color: #f9dad0;
    font-size: 32rpx;
}

.vipWord {
    color: #fff;
    font-size: 28rpx;
}

.vipPrivilege {
    color: #8383a2;
    font-size: 24rpx;
}

.clearfix:after {
    clear: both;
    content: " ";
    display: block;
    font-size: 0;
    height: 0;
    visibility: hidden;
}

.listItem {
    border-radius: 10rpx;
    color: #fff;
    float: left;
    margin-bottom: 40rpx;
    padding: 20rpx;
    width: calc(49% - 10rpx);
}

.listItem:nth-child(2n) {
    margin-left: 20rpx;
}

.listItem:nth-child(2n+1) {
    margin-left: 1%;
}

.firstHead {
    display: block;
    font-size: 32rpx;
    font-weight: 600;
    margin-bottom: 10rpx;
}

.nowBody {
    align-items: stretch;
    justify-content: flex-start;
}

.bg9C8EFD {
    background: #9c8efd;
}

.bgFFC078 {
    background: #ffc078;
}

.bg67BAFC {
    background: #67bafc;
}

.bgFE9786 {
    background: #fe9786;
}

.bottomItem {
    align-items: center;
    border: 1px solid #ccc;
    border-radius: 10rpx;
    border-radius: 20rpx;
    color: #fff;
    display: flex;
    float: left;
    justify-content: flex-start;
    margin-bottom: 40rpx;
    padding: 20rpx;
    width: calc(49% - 10rpx);
}

.bottomItem:nth-child(2n) {
    margin-left: 20rpx;
}

.bottomImg {
    display: inline-block;
    height: 60rpx;
    margin-right: 20rpx;
    width: 60rpx;
}

.bottomOne,.bottomTwo {
    color: #fff;
}

.pic-padding {
    padding: 4px;
}

.tower-swiper .tower-item {
    margin-left: calc(var(--left) * 100rpx - 150rpx);
    transform: scale(calc(.5 + var(--index) / 10));
    z-index: var(--index);
}

.water-teach {
    margin-top: 10px;
    text-align: center;
}

.sub-title {
    color: #7a7a7a;
    font-size: 24rpx;
    padding-top: 18rpx;
}

.tui-nickname {
    align-items: center;
    color: #13227a;
    display: flex;
    justify-content: center;
    text-decoration: underline;
}

.share-btn,.tui-nickname {
    font-size: 30rpx;
    font-weight: 500;
}

.share-btn {
    background: #000;
    color: red;
}

.tui-img-vip {
    height: 30rpx;
    margin-right: 18rpx;
    width: 60rpx;
}

.tui-notice-board {
    align-items: center;
    background: orange;
    box-sizing: border-box;
    display: flex;
    font-size: 28rpx;
    height: 48rpx;
    padding-right: 30rpx;
    top: 0;
    width: 100%;
    z-index: 999;
}

.tui-icon-bg {
    background: grey;
    padding-left: 30rpx;
    position: relative;
    z-index: 10;
}

.tui-icon-class {
    margin-right: 12rpx;
}

.tui-scorll-view {
    color: #fff;
    flex: 1;
    line-height: 1;
    overflow: hidden;
    white-space: nowrap;
}

.tui-notice {
    transform: translateX(100%);
}

.tui-animation {
    animation: tui-rolling 12s linear infinite;
}

@-webkit-keyframes tui-rolling {
    0% {
        transform: translateX(100%);
    }

    100% {
        transform: translateX(-170%);
    }
}

@keyframes tui-rolling {
    0% {
        transform: translateX(100%);
    }

    100% {
        transform: translateX(-170%);
    }
}

.tui-subject {
    box-sizing: border-box;
    font-size: 32rpx;
    font-weight: 700;
    padding: 100rpx 30rpx 30rpx;
}

.shadow-blur {
    box-shadow: 2rpx 4rpx 8rpx 2rpx rgba(0,0,0,.3);
}

.headImgWrap {
    align-items: center;
    background: #ccc;
    display: inline-flex;
    justify-content: center;
    padding: 10rpx 15rpx;
}

.headWord {
    height: 80px;
    padding: 30rpx 0 30rpx 26rpx;
}

.indexTitle {
    align-items: center;
    border-left: 8rpx solid #333;
    color: #333;
    display: flex;
    font-size: 34rpx;
    font-weight: 700;
    justify-content: space-between;
    margin: 10rpx 20rpx 10rpx 0;
    padding-left: 10rpx;
}

.cu-list.grid>.cu-item uni-text {
    color: #333;
}

.setIcon {
    height: 50rpx;
    width: 50rpx;
}

.prizeBtn::after {
    border: none;
}

.warnTip {
   font-size: 20rpx;
    border-radius: 12rpx;
    line-height: 52rpx;
    padding: 12rpx 20rpx;
    right: 20rpx;
    top: 38rpx;
    width: 400rpx;
    z-index: 2;
}

.closeIcon,.warnTip {
    position: absolute;
}

.closeIcon {
    background: #eaeaea;
    border-radius: 50%;
    bottom: 20rpx;
    font-size: 48rpx;
    height: 60rpx;
    line-height: 60rpx;
    right: 10rpx;
    text-align: center;
    width: 60rpx;
}

.triangle {
    border: 20rpx solid transparent;
    display: block;
    height: 0;
    left: calc(65% - 20rpx);
    position: absolute;
    top: -32rpx;
    width: 0;
}

.newInputWrap {
    margin: 0 auto;
    padding: 4px;
    width: 98%;
}

.nowInput {
    background-color: #f3fbee;
    border: 1px solid #1abaae;
    border-radius: 5px;
    height: 70rpx;
    margin-right: 10rpx;
    padding-left: 8rpx;
}

.bgGreen {
    background: #1abaae!important;
}

.mr5 {
    margin-right: 10rpx;
}

.text-center {
    display: block;
    font-size: 24rpx;
    margin: 15rpx 0;
}

.radius {
    border-radius: 20rpx;
}

.nowBody {
    align-items: center;
    display: flex;
    justify-content: space-around;
}

.headImg {
    height: 70rpx;
    width: 70rpx;
}

.shareBtn {
    color: #fff;
    height: 35px;
    line-height: inherit;
    padding: 12rpx 30rpx;
}

.fuIcon {
    height: 70rpx;
    margin-right: 20rpx;
    width: 70rpx;
}

.shareWrap {
    background: #add8e6;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    margin: 20rpx auto;
    padding: 12rpx 26rpx;
    width: 94%;
}

.fuIconWrap,.shareWrap {
    align-items: center;
}

.fuIconWrap {
    display: inline-flex;
    justify-content: flex-start;
}

.fuIconWrap>view {
    font-size: 28rpx;
    width: 80%;
}

.width98 {
    margin: 0 auto;
    width: 98%;
}