.container {
  padding: 20px;
}
.image-swiper {
  width: 560rpx;
  height: 700rpx;
  margin: 40rpx auto;
}

.gallery-image {
  width: 100%;
  height: 100%;
}
.result-image {
  width: 100%;
  height: auto;
  margin-bottom: 10px;
}

.result-title {
  font-size: 15px;
  font-weight: bold;
  margin-bottom: 20px;
  user-select: text;
}

.button-container button {
  margin-bottom: 10px;
}

.gallery {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.gallery-video {
  padding: 10px 0;
  width: 100%;
  display: flex;
  justify-content: center;
}
