.btn_div {
    margin: 150px auto 0;
    text-align: center;
}

.btn_div image {
    height: 100px;
    width: 100px;
}

.btn_div text {
    color: #515151;
}

.preview-warp {
    background-color: #000;
    bottom: 0;
    left: 0;
    position: fixed;
    right: 0;
    top: 0;
    z-index: 999;
}

.preview-warp .preview-body {
    height: calc(100vh - 25px - 88rpx - 100rpx - var(--safe-area-inset-bottom));
    margin: 0 30rpx;
    position: relative;
}

.preview-warp .preview-body image {
    height: 100%;
    position: absolute;
    width: 100%;
}

.preview-warp .preview-foot {
    align-items: center;
    bottom: 70rpx;
    display: flex;
    justify-content: center;
    position: absolute;
    width: 100%;
}

.preview-warp .preview-foot .view-btn {
    border: 1px solid #dfdfdf;
    border-radius: 30rpx;
    color: #fff;
    font-size: 14px;
    height: 60rpx;
    line-height: 60rpx;
    margin: 0 40rpx;
    text-align: center;
    width: 250rpx;
}

.preview-warp .preview-foot .bg {
    background-color: #006eff;
    border: 1px solid #006eff;
}

.content-tip {
    color: #515151;
    font-size: 12px;
    line-height: 20px;
    padding: 20px;
}

page {
    --safe-area-inset-top: 0px;
    --safe-area-inset-right: 0px;
    --safe-area-inset-bottom: 0px;
    --safe-area-inset-left: 0px;
}

@supports (top:constant(safe-area-inset-top)) {
    page {
        --safe-area-inset-top: constant(safe-area-inset-top);
        --safe-area-inset-right: constant(safe-area-inset-right);
        --safe-area-inset-bottom: constant(safe-area-inset-bottom);
        --safe-area-inset-left: constant(safe-area-inset-left);
    }
}

@supports (top:env(safe-area-inset-top)) {
    page {
        --safe-area-inset-top: env(safe-area-inset-top);
        --safe-area-inset-right: env(safe-area-inset-right);
        --safe-area-inset-bottom: env(safe-area-inset-bottom);
        --safe-area-inset-left: env(safe-area-inset-left);
    }
}